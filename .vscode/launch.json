    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: src/main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: drafts/main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/drafts/main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "test tests/unit/models/tests.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tests/unit/models/test_generate_sortings.py",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}